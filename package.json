{
  "name": "ricardobalk-personal-website",
  "version": "1.0.0",
  "description": "Vue-Jekyll Updated",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "./node_modules/.bin/concurrently \"cross-env NODE_ENV=development ./node_modules/.bin/webpack -w\" \"bundle exec jekyll serve -w --incremental\"",
    "htmlclean": "./node_modules/.bin/htmlclean dist -o dist",
    "build": "bundle exec jekyll build && cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress -p && npm run htmlclean",
    "serve": "./node_modules/.bin/serve ./dist -p 4000",
    "build:serve": "npm run build && npm run serve",
    "cleanup": "rm -rf node_modules/ dist/ src/assets/webpack/",
    "reset": "npm run cleanup && npm i"
  },
  "author": "Ricardo Balk",
  "repository": "https://github.com/ellipticcurv3/personal-website/",
  "license": "GPL-3.0+",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.0.0",
    "htmlclean-cli": "^1.0.9",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^7.1.0",
    "serve": "^11.0.2",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "foundation": "^4.2.1",
    "foundation-sites": "^6.5.3",
    "what-input": "^5.2.3"
  }
}
